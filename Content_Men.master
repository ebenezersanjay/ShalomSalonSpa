<%@ Master Language="C#" MasterPageFile="~/Site.master" AutoEventWireup="true" CodeFile="Content_Men.master.cs" Inherits="Content" %>

<asp:Content ID="BodyContent" ContentPlaceHolderID="MainContent" runat="Server">
    <div style="margin: 15px 0px 15px 0px; width: 100%; height:100%;">
        <asp:ContentPlaceHolder ID="ImageContent" runat="Server">
        </asp:ContentPlaceHolder>
    </div>
    <div class="col-md-4" style="margin: 5px 10px 10px 0px; width: 20%; min-height:450px; height: 100%;">
        <h2 style="margin-top: 10px;">Men</h2>
        <div style="cursor: pointer; margin-top: 20px" onclick="location.href='Services_Men_Skin.aspx';" onfocus="background: '#000000';">             
            <h4>Skin Care</h4>
            <ul class="list-unstyled">
                <li class="list-basic">Facials</li>
                <li class="list-basic">Waxing</li>
            </ul>
        </div>
        <div style="cursor: pointer; margin-top: 20px" onclick="location.href='Services_Men_Laser.aspx';" onfocus="background: '#000000';">
