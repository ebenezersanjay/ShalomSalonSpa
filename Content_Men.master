            <ul class="list-unstyled">
                <li class="list-basic">Cutting & Styling</li>
                <li class="list-basic">Color Services</li>
                <li class="list-basic">Eyebrow Trimming</li>
            </ul>
        </div>
          <div style="cursor: pointer; margin-top: 20px" onclick="location.href='Services_Men_Nail.aspx';" onfocus="background: '#000000';">
            <h4>Nails</h4>
            <ul class="list-unstyled">
                <li class="list-basic">Hand Care</li>
                <li class="list-basic">Foot Care</li>
            </ul>
        </div>
    </div>
    <div class="col-md-4" style="margin: 5px; width: 78%; height: 100%; min-height:450px;">
        <asp:ContentPlaceHolder ID="DescriptionContent" runat="Server">
        </asp:ContentPlaceHolder>
    </div>
</asp:Content>

